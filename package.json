{
  "name": "openapi-snippet-cli",
  "description": "Adds openapi snippets using openapi-snippet module in redoc style",
  "version": "0.2.1",
  "author": "Richard Kabiling",
  "bin": {
    "openapi-snippet": "./bin/run"
  },
  "bugs": "https://github.com/richardkabiling/openapi-snippet-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.15",
    "openapi-snippet": "^0.11.0",
    "openapi-types": "^1.3.5",
    "swagger-parser": "^9.0.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/js-yaml": "^3.12.4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/swagger-parser": "^7.0.1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/richardkabiling/openapi-snippet-cli",
  "keywords": [
    "openapi",
    "swagger",
    "snippets",
    "openapi-snippets",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "openapi-snippet"
  },
  "repository": "richardkabiling/openapi-snippet-cli",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
